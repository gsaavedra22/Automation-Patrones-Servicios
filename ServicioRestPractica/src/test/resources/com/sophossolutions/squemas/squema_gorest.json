{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "id",
        "name",
        "email",
        "gender",
        "status"
    ],
    "properties": {
        "id": {
            "type": "integer",
            "default": 0,
            "title": "The id Schema",
            "examples": [
                1028032
            ]
        },
        "name": {
            "type": "string",
            "default": "",
            "title": "The name Schema",
            "examples": [
                "geral"
            ]
        },
        "email": {
            "type": "string",
            "default": "",
            "title": "The email Schema",
            "examples": [
                "geralsaaveslss@gmail.com"
            ]
        },
        "gender": {
            "type": "string",
            "default": "",
            "title": "The gender Schema",
            "examples": [
                "female"
            ]
        },
        "status": {
            "type": "string",
            "default": "",
            "title": "The status Schema",
            "examples": [
                "active"
            ]
        }
    },
    "examples": [{
        "id": 1028032,
        "name": "geral",
        "email": "geralsaaveslss@gmail.com",
        "gender": "female",
        "status": "active"
    }]
}