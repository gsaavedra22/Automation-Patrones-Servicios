{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "name",
        "job",
        "id",
        "createdAt"
    ],
    "properties": {
        "name": {
            "type": "string",
            "default": "",
            "title": "The name Schema",
            "examples": [
                "morpheus"
            ]
        },
        "job": {
            "type": "string",
            "default": "",
            "title": "The job Schema",
            "examples": [
                "leader"
            ]
        },
        "id": {
            "type": "string",
            "default": "",
            "title": "The id Schema",
            "examples": [
                "882"
            ]
        },
        "createdAt": {
            "type": "string",
            "default": "",
            "title": "The createdAt Schema",
            "examples": [
                "2023-03-14T13:10:03.907Z"
            ]
        }
    },
    "examples": [{
        "name": "morpheus",
        "job": "leader",
        "id": "882",
        "createdAt": "2023-03-14T13:10:03.907Z"
    }]
}